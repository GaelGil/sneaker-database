<%- contentFor('title') %>
<%- contentFor('body') %>


<!-- display all shoes here -->


<div class="container">
<% for (let sneaker of sneakers) { %>

     <!-- <div id="column"> -->

      <div class="card">
        <img src="<%= sneaker.imageUrl %>" class="card-img-top" alt="sneakerImage">
          <div class="card-body">
            <h5 class="card-title"><%= sneaker.sneakerName %></h5>
            <a href="/sneakers/<%= sneaker.id %>/detail" class="btn btn-dark">More</a>
        </div>
      </div>

      <br>
      <br>
    <!-- </div> -->
     <% } %>

</div>


 

